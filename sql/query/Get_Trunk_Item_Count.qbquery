select t.dwAccountID, a.sAccountClubID, from_unixtime(a.ftRegisterDate/1000) as RegisteredDate, t.nItemID, SUM(t.nNumber) as TotalCount, a.bBlocked, a.nBlockReason, a.nGradeCode from schema0.trunkbundle t INNER JOIN schema.account a ON a.dwaccountid = t.dwaccountid
where t.nitemid = 4001878 AND nBlockReason = 0 group by t.dwaccountid order by TotalCount desc